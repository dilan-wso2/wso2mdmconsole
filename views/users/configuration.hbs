<div class="container-fluid">

	<div class="raw-fluid">
		<div class="span12">{{>config-navigation}}</div>
	</div>

	<div class="raw">
		<div class="span12">
			<div class="raw-fluid">

				<div class="block">
					<div class="block-header">
						<div class="block-header-title pull-left">Users</div>
						<div class="pull-right">
							<a class="btn btn-orange" href="add"><i
								class="icon-plus icon-white"> </i> Add User</a>
						</div>
					</div>

					<div class="block-body block-body container-fluid raw-fluid">		

						
						<table cellpadding="0" cellspacing="0" border="0"
							class="table table-striped table-bordered" id="main-table">
							<thead>
								<tr>
									<th>First Name</th>
									<th>Last Name</th>
									<th>e Mail</th>
									<th>Mobile</th>
									<th>Action</th>
								</tr>
							</thead>
							<tbody>
								{{#data.users}}
								<tr>
									<td>{{first_name}}</td>
									<td>{{last_name}}</td>
									<td>{{username}}</td>
									<td>{{mobile_no}}</td>
									<td class="center"><a href="edit" title="Edit"
										class="icon-edit"> </a><a href="edit" title="Delete"
										class="icon-remove"> </a></td>
								</tr>
								{{/data.users}}
							</tbody>
						</table>




					</div>

				</div>

			</div>

		</div>

	</div>

</div>

