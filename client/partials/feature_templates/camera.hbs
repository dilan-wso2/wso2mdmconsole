<div class="feature-dialog">

	<div class="modal-header">
		<h4 id="myModalLabel">Device Camera</h4>
	</div>
	<div class="modal-body">
		
		<div class="raw well-small">
			<button  data-dismiss="modal" aria-hidden="true" class="btn btn-orange feature-command" data-value="Enable">Enable</button>
		</div>
		
		<div class="raw well-small">
			<button  data-dismiss="modal" aria-hidden="true" class="btn btn-danger feature-command" data-value="Disable">Disable</button>
		</div>	

	</div>

</div>
